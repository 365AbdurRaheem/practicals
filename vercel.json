{
  "version": 2,
  "name": "practicals",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.htm"
    },
    {
      "src": "/fb",
      "dest": "/"
    }
  ],
  {
  "functions": {
    "app/api/**/*": {
      "maxDuration": 5
    }
  }
}
}
