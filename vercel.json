{
  "version": 2,
  "name": "practicals",
   "functions": {
    "myFunction": {
      "maxDuration": 30 // or any other timeout value in seconds
    }
  }
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.htm"
    },
    {
      "src": "/fb",
      "dest": "/"
    }
  ]
}
